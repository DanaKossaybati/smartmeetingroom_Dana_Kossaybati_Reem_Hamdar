							"name": "Success - Admin View",
							"originalRequest": {
								"method": "GET",
								"header": [],
								"url": {
									"raw": "{{base_url}}/api/analytics/bookings/summary",
									"host": ["{{base_url}}"],
									"path": ["api", "analytics", "bookings", "summary"]
								}
							},
							"status": "OK",
							"code": 200,
							"_postman_previewlanguage": "json",
							"header": [
								{
									"key": "Content-Type",
									"value": "application/json"
								}
							],
							"body": "{\n    \"success\": true,\n    \"summary\": {\n        \"total_bookings\": 45,\n        \"bookings_this_month\": 18,\n        \"bookings_last_month\": 15,\n        \"growth_rate_percent\": 20.0,\n        \"status_breakdown\": {\n            \"confirmed\": 32,\n            \"cancelled\": 8,\n            \"completed\": 5\n        },\n        \"average_bookings_per_user\": 4.5,\n        \"top_users\": []\n    },\n    \"scope\": \"all_bookings\",\n    \"generated_at\": \"2025-11-24T14:30:00.123456\"\n}"
						},
