		{
			"name": "Test Data Setup",
			"item": [
				{
					"name": "Login to Get JWT Token",
					"event": [
						{
							"listen": "test",
							"script": {
								"exec": [
									"// Save JWT token to environment variable",
									"if (pm.response.code === 200) {",
									"    const response = pm.response.json();",
									"    pm.environment.set('jwt_token', response.access_token);",
									"    console.log('JWT token saved:', response.access_token);",
									"}"
								],
								"type": "text/javascript"
							}
						}
					],
					"request": {
						"method": "POST",
						"header": [
							{
								"key": "Content-Type",
								"value": "application/json"
							}
						],
						"body": {
							"mode": "raw",
							"raw": "{\n    \"username\": \"admin\",\n    \"password\": \"admin123\"\n}"
						},
						"url": {
							"raw": "http://localhost:8001/api/users/login",
							"protocol": "http",
							"host": ["localhost"],
							"port": "8001",
							"path": ["api", "users", "login"]
						},
						"description": "Login to Users Service to obtain JWT token.\n\nThe token is automatically saved to the {{jwt_token}} variable and used for all analytics requests.\n\nDefault credentials:\n- Username: admin\n- Password: admin123"
					},
					"response": []
				}
			],
			"description": "Setup requests to prepare for testing"
